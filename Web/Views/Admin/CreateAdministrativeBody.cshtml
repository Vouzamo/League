@using Core.Models.Classes
@model AdministrativeBody

@{
    Layout = "~/Views/Shared/_Admin.cshtml";
}

@section Title
{
    Admin &gt; New Administrative Body
}

<div class="row">
    <div class="col-md-12">
        <ol class="breadcrumb">
            <li>@Html.ActionLink("Admin", "Index")</li>
            <li class="active">New Administrative Body</li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Administrative Body</h3>
            </div>
            <div class="panel-body">
                @Html.Partial("_AdministrativeBodyForm", Model)
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        @Html.ActionLink("Back to Administrative Bodies", "Index")
    </div>
</div>
