@using Core.Models.Classes
@model Division

@{
    Layout = "~/Views/Shared/_Admin.cshtml";
}

@section Title
{
    Admin &gt; New Division
}

<div class="row">
    <div class="col-md-12">
        <ol class="breadcrumb">
            <li>@Html.ActionLink("Admin", "Index")</li>
            <li class="active">New Division</li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Division</h3>
            </div>
            <div class="panel-body">
                @Html.Partial("_DivisionForm", Model)
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        @Html.ActionLink("Back to Divisions", "Season", null, new { id = Model.SeasonId })
    </div>
</div>